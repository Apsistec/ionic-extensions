{
  "name": "@co.mmons/ionic-extensions",
  "version": "2.0.0",
  "author": "co.mmons",
  "private": false,
  "license": "MIT",
  "$schema": "./node_modules/ng-packagr/package.schema.json",
  "scripts": {
    "build": "npx ng-packagr -p package.json && node-utils-file-system-glob-copy src dist '**/package.json' && rm -R dist/bundles dist/esm5 dist/fesm5 dist/fesm2015 > /dev/null",
    "build:select": "npx ng-packagr -p package.select.json",
    "build-old": "rm -Rf dist/**/* && ngc && node-utils-file-system-glob-copy src dist '**/*.scss'",
    "build-copy": "node-utils-file-system-glob-copy src dist '**/*.scss'",
    "install": "node-utils-npm-install-dist"
  },
  "peerDependencies": {
    "@co.mmons/angular-extensions": "git+https://github.com/co-mmons/angular-extensions.git",
    "@co.mmons/angular-intl": "git+https://github.com/co-mmons/angular-intl.git",
    "@co.mmons/js-utils": "git+https://github.com/co-mmons/js-utils.git",
    "@co.mmons/node-utils": "git+https://github.com/co-mmons/node-utils.git",
    "@co.mmons/rxjs-utils": "git+https://github.com/co-mmons/rxjs-utils.git",
    "@co.mmons/web-utils": "git+https://github.com/co-mmons/web-utils.git",
    "@angular/cdk": "^8.0.1",
    "@angular/common": "^8.0.3",
    "@angular/compiler": "^8.0.3",
    "@angular/compiler-cli": "^8.0.3",
    "@angular/core": "^8.0.3",
    "@angular/forms": "^8.0.3",
    "@angular/platform-browser": "^8.0.3",
    "@angular/platform-browser-dynamic": "^8.0.3",
    "@angular/platform-server": "^8.0.3",
    "@angular/router": "^8.0.3",
    "@ionic/angular": "^4.6.0",
    "dragula": "^3.7.2",
    "rxjs": "^6.5.2",
    "zone.js": "^0.9.1"
  },
  "devDependencies": {
    "@capacitor/core": "^1.1.0",
    "@types/dragula": "^2.1.34",
    "bignumber.js": "^9.0.0",
    "ng-packagr": "^5.5.0",
    "typescript": "~3.4.5"
  },
  "main": "esm2015/co.mmons-ionic-extensions.js",
  "typings": "co.mmons-ionic-extensions.d.ts",
  "metadata": "co.mmons-ionic-extensions.metadata.json",
  "sideEffects": false,
  "ngPackage": {
    "lib": {
      "entryFile": "src/modules.ts"
    }
  }
}
