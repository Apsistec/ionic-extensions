    "@types/prosemirror-schema-basic": "^1.0.1",
    "@types/prosemirror-schema-list": "^1.0.1",
    "@types/prosemirror-state": "^1.2.3",
    "@types/prosemirror-view": "^1.3.2",
    "@types/yargs": "^13.0.2",
    "bignumber.js": "^9.0.0",
    "fs-extra": "^8.1.0",
    "ng-packagr": "^5.7.1",
    "ts-node": "^8.3.0",
    "typescript": "~3.4.5",
    "yargs": "^14.0.0"
  },
  "dependencies": {
    "@types/dragula": "^2.1.34",
