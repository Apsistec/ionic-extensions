{
  "name": "@co.mmons/ionic-extensions",
  "version": "2.0.0",
  "author": "co.mmons",
  "private": false,
  "license": "MIT",
  "scripts": {
    "build": "rm -Rf dist/**/* && npx ts-node --project tsconfig.build.json build.ts",
    "build:module": "npx ts-node --project tsconfig.build.json build.ts",
    "intl": "npm run intl:datetime && npm run intl:html-editor",
    "intl:datetime": "js-intl-google-sheet-importer --output-path src/datetime-picker/i18n --output-type json --document 1lTpcwU0ubFX7bJFCt8zeHYrwbDaoxNcjzpjUVdmiY_w --worksheet 1 --filterTags ionic-extensions-datetime",
    "intl:html-editor": "js-intl-google-sheet-importer --output-path src/html-editor/i18n --output-type json --document 1lTpcwU0ubFX7bJFCt8zeHYrwbDaoxNcjzpjUVdmiY_w --worksheet 1 --filterTags ionic-extensions-html-editor",
    "install": "node-utils-npm-install-dist"
  },
  "peerDependencies": {
    "@co.mmons/angular-extensions": "git+https://github.com/co-mmons/angular-extensions.git",
    "@co.mmons/angular-intl": "git+https://github.com/co-mmons/angular-intl.git",
    "@co.mmons/js-intl": "git+https://github.com/co-mmons/js-intl.git",
    "@co.mmons/js-utils": "git+https://github.com/co-mmons/js-utils.git",
    "@co.mmons/node-utils": "git+https://github.com/co-mmons/node-utils.git",
    "@co.mmons/rxjs-utils": "git+https://github.com/co-mmons/rxjs-utils.git",
    "@co.mmons/web-utils": "git+https://github.com/co-mmons/web-utils.git",
    "@angular/cdk": "^8.0.1",
    "@angular/common": "^8.0.3",
    "@angular/compiler": "^8.0.3",
    "@angular/compiler-cli": "^8.0.3",
    "@angular/core": "^8.0.3",
    "@angular/forms": "^8.0.3",
    "@angular/platform-browser": "^8.0.3",
    "@angular/platform-browser-dynamic": "^8.0.3",
    "@angular/platform-server": "^8.0.3",
    "@angular/router": "^8.0.3",
    "@ionic/angular": "^4.6.0",
    "rxjs": "^6.5.2",
    "zone.js": "^0.9.1"
  },
  "devDependencies": {
    "@capacitor/core": "^1.1.0",
    "@types/fs-extra": "^8.0.0",
    "@types/prosemirror-commands": "^1.0.1",
    "@types/prosemirror-dropcursor": "^1.0.0",
    "@types/prosemirror-gapcursor": "^1.0.1",
